@model IEnumerable<RoyalHalls1.Models.About>

@{
    ViewData["Title"] = "AbountUs_Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.AP = ViewBag.adminProfile;

}




<div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="page-breadcrumb bg-white">
                <div class="row align-items-center">
                    
                    
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- ============================================================== -->
            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-sm-12">
                        <div class="white-box">
                            <h3 class="box-title">About us</h3>
                            <div class="table-responsive">  


<h1>About us</h1>






<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descriotion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Photo)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descriotion)
            </td>
            
            <td>
                <img src=@Url.Content("~/Image/" + item.Photo) 
                 asp-append-version ="true" style="border-radius:20%;" width="100" height="100" />
            </td>

            
            <td>
                <a  class="btn btn-primary"  asp-action="AboutUs_Edit" asp-route-id="@item.Id">Edit</a> 
            </td>

        </tr>
}
    </tbody>
</table>


                            </div>
                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- End PAge Content -->
                <!-- ============================================================== -->
                <!-- ============================================================== -->
                <!-- Right sidebar -->
                <!-- ============================================================== -->
